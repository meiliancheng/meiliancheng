    "for-editor": "^0.3.5",
    "highlight.js": "^10.7.2",
    "mobx": "^6.2.0",
    "mobx-react-lite": "^3.2.0",
    "nprogress": "^0.2.0",
    "postcss-px2rem": "^0.3.0",
    "react-infinite-scroller": "^1.2.4",
    "umi": "^3.4.8",
    "viewerjs": "^1.9.0"
